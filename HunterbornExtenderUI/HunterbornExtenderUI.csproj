    </Content>
    <Content Include="Plugins\Skyrim Immersive Creatures Special Edition.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Plugins\Skyrim.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Plugins\SkyTEST-RealisticAnimals&amp;Predators.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Extended.Wpf.Toolkit" Version="4.4.0" />
    <PackageReference Include="Mutagen.Bethesda" Version="0.39.6" />
    <PackageReference Include="Mutagen.Bethesda.Json" Version="0.39.6" />
