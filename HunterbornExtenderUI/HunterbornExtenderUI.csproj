    <None Remove="Plugins\DiverseDragonsCollectionSE.json" />
    <None Remove="Plugins\Dragonborn.json" />
    <None Remove="Plugins\Gray Fox Cowl.json" />
    <None Remove="Plugins\moonpath.json" />
    <None Remove="Plugins\Skyrim Immersive Creatures Special Edition.json" />
    <None Remove="Plugins\Skyrim.json" />
    <None Remove="Plugins\SkyTEST-RealisticAnimals&amp;Predators.json" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Plugins\Complete Alchemy &amp; Cooking Overhaul.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Plugins\Dawnguard.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Plugins\DiverseDragonsCollectionSE.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
